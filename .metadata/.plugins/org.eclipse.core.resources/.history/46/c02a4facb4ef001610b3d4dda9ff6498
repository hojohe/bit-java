/* 로그인상태유지 체크박스 클릭시 */
$("#autoType").on("click", function() {

console.log("dochk들어옴");
	if($(this).attr("checked")) {
		$(this).attr("checked", false);
		$("#autoType").val("N");
	} else {
		$(this).attr("checked", true);
		$("#autoType").val("Y");
	}
	if($(this).attr("checked")) {
	
	} 
console.log("value = " + $("#autoType").val());
});

