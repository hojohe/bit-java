<mapper namespace="kr.co.mlec.board.dao.BoardMapper">
	<select id="" resultMap="">
		select no, 
			   title, 
			   writer, 
			   content, 
			   reg_date
		  from tb_board
		 order by reg_date desc
	</select>
	
</mapper>