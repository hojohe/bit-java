	<button>요청</button>
	<div id="result"></div>
	<script>
	$("button").on("click", function () {
		$.ajax({
			url: "/09_jquery/ajax/exam02",
			type: "GET",
			data: {"msg" : $("#msg").val()}
		}).done(function (msg) {
			$("#result").html(msg);
		});
	});
	</script>
