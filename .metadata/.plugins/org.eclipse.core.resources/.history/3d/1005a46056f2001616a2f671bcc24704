select *
		  from t_feed
		 where user_no = 42
		 
		 update T_FEED
		 set conf_yn = 'N'
		 WHERE USER_NO = 42
		   and feed_type = 2
select * from t_feed_Detail

select feed_no,
			   user_no,
		       end_date,
		       feed_dday
		  from (
		  	select d.feed_no,
		  		   d.user_no,
		 	       f.end_date,
		 	       f.feed_dday
		   	  from t_feed_detail d, t_feed f
		  	 where d.feed_no = f.feed_no
		  	   and d.user_no = f.user_no
		  	   and d.user_no = 42
			order by days
		)
		where rownum = 1
		
select feed_no, user_no, end_date, feed_dday, reg_date
  from (
  		select d.feed_no, d.user_no, f.end_date, f.feed_dday, f.reg_date
  		  from t_feed f, t_feed_detail d
  		 where 1=1
  		   and f.feed_no = d.feed_no
  		   and f.user_no = d.user_no
  		   and f.feed_type = 2
  		   and d.user_no = 42
  		   group by d.feed_no, d.user_no, f.end_date, f.feed_dday, f.reg_date
		   order by f.reg_date desc
  	   ) a
 where 1=1
   and rownum = 1

