select f.feed_no,
    		   f.user_no,
    		   f.bullet_no,
    		   f.feed_type,
    		   f.feed_dday,
    		   f.start_date,
    		   f.end_date,
    	       f.feed_title, 
    	       f.delete_yn,
    	       f.reg_date,
    	       f.conf_yn,
    	       b.bullet_img_path 
    	  from t_feed f, t_bullet b
    	 where f.bullet_no = b.bullet_no
    	   and f.user_no = 42

select percent
  from t_feed_detail
 where 1=1
   and user_no = 42
   and feed_no = 2
   and days = 29

