<title>Insert title here</title>
<script>

	// MyMap 객체 구현하기
	function funcLength() {
		var i = 0;
		if(key == "del") {
			--i;			
		}
		if(key == "put") {
			++i;			
		}
		return i;
	}
	
	function funcClear() {
		return MyMap = {};
	}
	var MyMap = {
		put: function(key, value) {
			if(key == "id") {
				this.id = value;
			}
			if(key == "name") {
				this.name = value;
			}
			length("put");
		},
		size: function() {
			this.length();
		},
		get: function(id) {
			return this.id;
		},
		del: function(key) {
			
			if(key == "id") {
				delete this.id;
			}
			if(key == "name") {
				delete this.name;
			}
			length("del");
			
		},
		clear: function() {
			delete this.name;
			//MyMap = new Object();
		}
		
	}; 
	
	var mData = MyMap;
	mData.put("id", "sbc");
	mData.put("name", "sonbyungchan");
	
	console.log(mData.size());      // 2
	console.log(mData.get("id"));   // sbc
	mData.del("id");          
	console.log(mData.get("id"));   // undefined	
	mData.clear();
	console.log(mData.size());      // 0
</script>
