select * from t_user
select * from t_feed
where feed_no = 48


delete from t_feed
where feed_no = 5

insert into t_feed (feed_no, user_no, bullet_no, feed_type, feed_title)
values (s_feed_feed_no.nextval, 2, 4, 2, '필라테스 수업' );


select * from sequence


select f.feed_no,
    		   f.user_no,
    		   f.bullet_no,
    		   f.feed_type,
    		   f.feed_dday,
    		   to_char(f.start_date, 'yyyy-mm-dd'),
    		   to_char(f.end_date, 'yyyy-mm-dd'),
    		   to_char(f.alert_date, 'yyyy-mm-dd'),
    		   f.alert_schedule,
    	       f.feed_title,
    	       f.feed_content,
    	       f.delete_yn,
    	       f.reg_date,
    	       b.bullet_img_path 
    	  from t_feed f, t_bullet b
    	 where f.bullet_no = b.bullet_no 
    	 	   and f.feed_no = 9