select feed_no,
       end_date
  from (
  	select d.feed_no,
 	       f.end_date
   	  from t_feed_detail d, t_feed f
  	 where d.feed_no = f.feed_no
  	   and d.user_no = f.user_no
  	   and d.user_no = 1
	order by days
)
where rownum = 1

update T_FEED
set feed_dday = 30
commit;
select * from t_feed
where feed_no = 4


update t_feed set end_date = sysdate + 3
where feed_no = 4
		
select * from T_FEED_DETAIL
update t_feed_detail set percent = 80 



select detail_no,
		   feed_no,
		   user_no,
		   days,
		   percent
	  from t_feed_detail
	 where feed_no = 4
	   and user_no = 1
	    order by days desc
	   
update t_feed_detail
   set percent = 0
 where days in (27,28,29,30);
 
 insert into t_user (user_no, id, pass, nick_name) 
values(s_user_user_no.nextVal, 'puj', '1234', '김신');
 
 
 
 
 
 select f.feed_no,
    		   f.user_no,
    		   f.bullet_no,
    		   f.feed_type,
    		   f.feed_dday,
    		   f.start_date,
    		   f.end_date,
    	       f.feed_title, 
    	       f.delete_yn,
    	       f.reg_date,
    	       b.bullet_img_path 
    	  from t_feed f, t_bullet b
    	 where f.bullet_no = b.bullet_no
    	 order by feed_type desc, end_date asc
    	 
    	 
    	 
    	 
select feed_no,
			   user_no,
		       end_date,
		       feed_dday,
		       percent
		  from (
		  	select d.feed_no,
		  		   d.user_no,
		 	       f.end_date,
		 	       f.feed_dday,
		 	       d.percent
		   	  from t_feed_detail d, t_feed f
		  	 where d.feed_no = f.feed_no
		  	   and d.user_no = f.user_no
		  	   and d.user_no = 1
			order by days
		)
		where rownum = 1    	 
select * from t_feed_detail



select * from t_user



