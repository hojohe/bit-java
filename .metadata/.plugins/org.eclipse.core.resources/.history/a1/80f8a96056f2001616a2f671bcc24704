select id , pass
	  from t_user
	 where id = 'nameA'
	 
	 
select f.feed_no,
    		   f.user_no,
    		   f.bullet_no,
    		   f.feed_type,
    		   f.feed_dday,
    		   f.start_date,
    		   f.end_date,
    	       f.feed_title, 
    	       f.delete_yn,
    	       f.reg_date,
    	       f.conf_yn,
    	       b.bullet_img_path 
    	  from t_feed f, t_bullet b
    	 where f.bullet_no = b.bullet_no
    	   and f.user_no = 45