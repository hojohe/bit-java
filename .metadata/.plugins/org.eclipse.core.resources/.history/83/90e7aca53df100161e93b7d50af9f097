package kr.co.mini5.vo;

import java.util.Date;

/**
 * feed를 표현하는 데이터 클래스
 *
 */
public class FeedVO {
	/** 피드 번호 */
	private int feedNo;
	/** 사용자 번호 */
	private int userNo;
	/** 불렛 기호 번호 */
	private int bulletNo;
	/** 종류(피드 or 스토리) */
	private int feedType;
	/** 디데이(계획일 30/50/100) */
	private int feedDday;
	/** 시작일(피드 실행일) */
	private String startDate;
	/** 종료일(계획 종료일) */
	private String endDate;
	/** 피드 제목 */
	private String feedTitle;
	/** 피드 내용 */
	private String feedContent;
	/** 등록일 */
	private Date regDate;
	/** 수정일 */
	private Date modDate;
	/** 삭제 여부 */
	private String deleteYn;
	/** 알림일 */
	private String alertDate;
	/** 알림일(매일/주간) */
	private int alertSchedule;
	/** 인증여부 */
	private String confYn;
	/** 인증 사진 */
	private String confImgPath;
	/** 인증 내용 */
	private String confContent;
	/** 인증 해시태그 */
	private String hashTag;
	
	private String bulletImgPath;
	private String bulletImgPath;
	
	public String getBulletImgPath() {
		return bulletImgPath;
	}
	public void setBulletImgPath(String bulletImgPath) {
		this.bulletImgPath = bulletImgPath;
	}
	public int getFeedNo() {
		return feedNo;
	}
	public void setFeedNo(int feedNo) {
		this.feedNo = feedNo;
	}
	public int getUserNo() {
		return userNo;
	}
	public void setUserNo(int userNo) {
		this.userNo = userNo;
	}
	public int getBulletNo() {
		return bulletNo;
	}
	public void setBulletNo(int bulletNo) {
		this.bulletNo = bulletNo;
	}
	public int getFeedType() {
		return feedType;
	}
	public void setFeedType(int feedType) {
		this.feedType = feedType;
	}
	public int getFeedDday() {
		return feedDday;
	}
	public void setFeedDday(int feedDday) {
		this.feedDday = feedDday;
	}
	public String getStartDate() {
		return startDate;
	}
	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}
	public String getEndDate() {
		return endDate;
	}
	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}
	public String getFeedTitle() {
		return feedTitle;
	}
	public void setFeedTitle(String feedTitle) {
		this.feedTitle = feedTitle;
	}
	public String getFeedContent() {
		return feedContent;
	}
	public void setFeedContent(String feedContent) {
		this.feedContent = feedContent;
	}
	public Date getRegDate() {
		return regDate;
	}
	public void setRegDate(Date regDate) {
		this.regDate = regDate;
	}
	public Date getModDate() {
		return modDate;
	}
	public void setModDate(Date modDate) {
		this.modDate = modDate;
	}
	public String getDeleteYn() {
		return deleteYn;
	}
	public void setDeleteYn(String deleteYn) {
		this.deleteYn = deleteYn;
	}
	public String getAlertDate() {
		return alertDate;
	}
	public void setAlertDate(String alertDate) {
		this.alertDate = alertDate;
	}
	public int getAlertSchedule() {
		return alertSchedule;
	}
	public void setAlertSchedule(int alertSchedule) {
		this.alertSchedule = alertSchedule;
	}
	public String getConfYn() {
		return confYn;
	}
	public void setConfYn(String confYn) {
		this.confYn = confYn;
	}
	public String getConfImgPath() {
		return confImgPath;
	}
	public void setConfImgPath(String confImgPath) {
		this.confImgPath = confImgPath;
	}
	public String getConfContent() {
		return confContent;
	}
	public void setConfContent(String confContent) {
		this.confContent = confContent;
	}
	public String getHashTag() {
		return hashTag;
	}
	public void setHashTag(String hashTag) {
		this.hashTag = hashTag;
	}
	
	

	
}
