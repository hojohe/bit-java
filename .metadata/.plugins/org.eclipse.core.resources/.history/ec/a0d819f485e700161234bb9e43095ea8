<title>Insert title here</title>
</head>
<body>
	<script>
		
		var i = 10;
		console.log("이현우바보 : " + i);
		
		var msg = "hi";
		console.log("ㅋㅋㅋㅋㅋ : " + msg);
		
		var bool = true;
		console.log("약오르냥 : " + bool);
		
		var nullVal = null;
		console.log("null : " + nullVal);
		
		var unVal;
		console.log("undefined : " + unVal);
		
		var obj = {};
		console.log("object : " + obj);
		
	</script>	
</body>
</html>