$("input[type=button]").click(function () {
	
	$("#divObj").html();
	$.ajax({
		url: "/09_jquery/sticky/save.do",
		type: "POST",
		data: {
			x:999,
			y:999,
			content:
		},
	});
	
});

$("#memo1").keyup(function () {
	
	$.ajax({
		url: "/09_jquery/sticky/edit.do",
		type: "POST",
		data: {
			x:123, 
			y:123, 
			id:1,
			content:$("#memo1").html()
		}
	})
	
});
