<body>
	<p>
		<a id="" onclick="funcSild();">슬라이드</a>
	</p>

	<div id="div1">슬라이드 1번</div>
	<div id="div2">슬라이드 2번</div>
	<div id="div3">슬라이드 3번</div>
	<div id="div4">슬라이드 4번</div>
	<div id="div5">슬라이드 3번</div>

	<script>
		var bottom = 400;
		var divObj = document.querySelector("div");
		
		function funcSild() {
			id = setInterval(function() {
				bottom += 16;
				divObj.style.clip = "rect(0px, 500px, " + bottom + "px, 0px)";
				if(bottom == 160) {
					clearInterval(id);
				}
			}, 100);
			
			
		}
	</script>
