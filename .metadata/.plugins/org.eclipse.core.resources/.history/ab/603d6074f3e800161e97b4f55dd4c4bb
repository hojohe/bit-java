<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>

	// MyMap 객체 구현하기
	function funcId(id) {
		return {id: id};
	}
	function funcName(name) {
		return {name: name};
	}
	
	function funcClear() {
		var MyMap = new MyMap();
	}
	
	var MyMap = {
		put: function(key, value) {
			funcId(value);
			funcName(value);
		},
		size: function() {
			return this.put.length;
		},
		get: function(id) {
			return this.id;
		},
		del: function(id) {
			delete this.id;
		},
		clear: function() {
			funcClear();
		}
	}; 
	
	var mData = MyMap;
	mData.put("id", "sbc");
	mData.put("name", "sonbyungchan");
	
	console.log(mData.size());      // 2
	console.log(mData.get("id"));   // sbc
	mData.del("id");          
	console.log(mData.get("id"));   // undefined	
	mData.clear();
	console.log(mData.size());      // 0
</script>
</head>
<body>

</body>
</html>